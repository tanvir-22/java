import java.util.Scanner;
public class q {
    public static void main(String[] args){
        Scanner input=new Scanner(System.in);
        System.out.println("enter a number : ");
        int a= input.nextInt();
        System.out.println("enter another number : ");
        int b=input.nextInt();
        int c=a+b;
        System.out.println( +a+ "+" +b+ "="+c);
    }
}
