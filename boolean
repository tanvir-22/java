public class q {
    public static void main(String[] args){
     int a=7;
     int b=23;
     boolean m = a>b;
     if (m)
     {
         System.out.println("a>b");
     }
     else
     {
         System.out.println("a<b");
     }
    }
}





import java.util.Scanner;
public class q {
    public static void main(String[] argc){
        Scanner input=new Scanner(System.in);
        System.out.println("enter a number : ");
        int a= input.nextInt();
        System.out.println("enter another number : ");
        int b=input.nextInt();
        boolean isAequalB= a==b;
        System.out.println("isAequalB="+isAequalB);
        boolean isAnotequalB= a!=b;
        System.out.println("isAnotequalB="+isAnotequalB);
        boolean isAgreaterorequaltoB= a>=b;
        System.out.println("isAgreaterorequaltoB="+isAgreaterorequaltoB);
        boolean isAlessthanorequaltoB=a<=b;
        System.out.println("isAlessthanorequaltoB="+isAlessthanorequaltoB);
    }
}
